proxies:
proxy-groups:
  - name: PROVIDER
    type: select
    proxies:
      - BEST PING
      - LOAD-BALANCE
    use:
      - AKUN
  - name: BEST PING
    type: url-test
    url: http://www.gstatic.com/generate_204
    interval: 300
    use:
      - AKUN
  - name: LOAD-BALANCE
    type: load-balance
    strategy: random
    respect-weight: true
    weight: 2
    url: http://www.gstatic.com/generate_204
    interval: 300
    use:
      - AKUN
proxy-providers:
  AKUN:
    type: file
    path: ./account.yaml
    health-check:
      enable: true
      lazy: true
      url: http://www.gstatic.com/generate_204
      interval: 300
rules:
  - GEOSITE,CATEGORY-ADS-ALL,REJECT
  - MATCH,PROVIDER